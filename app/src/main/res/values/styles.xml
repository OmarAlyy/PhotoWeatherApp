<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
         <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="fontFamily">@font/din_next</item>
        <item name="android:fontFamily">@font/din_next</item>
    </style>

    <style name="ThemeWhite" parent="AppTheme">
        <item name="colorAccent">@color/blue</item>
        <item name="colorPrimaryDark">@color/blue</item>
        <item name="colorPrimary">@color/blue</item>
    </style>



    <style name="AlertDialogTheme" parent="ThemeOverlay.MaterialComponents.MaterialAlertDialog"></style>
</resources>